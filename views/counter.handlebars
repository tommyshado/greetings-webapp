<div class="row">
    {{#each name}}
    <h1>{{name}} number of greetings</h1>

    <table class="table table-striped">
        
        <tr>
            <th scope="col">Username</th>
            <th scope="col">Number of greet</th>
        </tr>

        <tr>
            <td>{{name}}</td>
            <td>{{count}}</td>
        </tr>

    </table>
    {{/each}}
    
</div>