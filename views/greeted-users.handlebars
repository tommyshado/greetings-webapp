<div class="row">
    <h1>Greeted users data</h1>

    <table class="table table-striped">
        
        <tr>
            <th>Username</th>
            <th>Number of greet</th>
        </tr>

        {{#each greetedUsers}}
        <tr>
            <td><a href="/counter/{{username}}">{{username}}</a></td>
            <td>{{counter}}</td>
        </tr>
        {{/each}}

    </table>
    
</div>